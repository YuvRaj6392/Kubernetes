- kind create cluster --config clusters.yml --name local
- cat  ~/.kube/config
- kubectl get nodes
- kubectl run nginx --image=nginx --port=80
- kubectl describe pod nginx
- kubectl apply -f manifest.yml
- kubectl apply -f deployment.yml
- kubectl get deployment
- kubectl get rs
- kubectl rollout history deployment/nginx-deployment(history of deployment)
- kubectl rollout undo deployment/nginx-deployment(undo last deployment)
- kubectl logs -f podname
- kubectl get pods -owide(get pod ip)
- kubectl get svc
- kubectl get pods --all-namespaces
- kubectl get namespaces
- kubectl create namespace backend
- kubectl get deployment --namespace backend
- kubectl config set-context --current --namespace=backend-team
- kubectl get pods
- kubectl config set-context --current --namespace=default
- helm repo add ingress-nginx https://kubernetes.github.io/ingress-nginx
- helm repo update
- helm install nginx-ingress ingress-nginx/ingress-nginx --namespace ingress-nginx --create-namespace
- Navigate to C:\Windows\System32\drivers\etc.
  Set the file type to "All Files" and select the hosts file.(DNS spoofing)